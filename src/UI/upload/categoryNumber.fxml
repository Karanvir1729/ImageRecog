      <font>
         <Font size="21.0" />
      </font>
   </Label>
   <Button fx:id="select" layoutX="365.0" layoutY="205.0" mnemonicParsing="false" onAction="#loadCategoryNames" text="Enter">
      <font>
         <Font size="16.0" />
      </font>
   </Button>
   <TextField fx:id="categories" layoutX="131.0" layoutY="205.0">
      <font>
         <Font size="16.0" />
      </font>
   </TextField>
   <Label fx:id="errorLabel" layoutX="79.0" layoutY="265.0" prefHeight="21.0" prefWidth="420.0">
      <font>
         <Font size="15.0" />
      </font>
   </Label>
   <Button fx:id="back" layoutX="14.0" layoutY="350.0" mnemonicParsing="false" onAction="#back" prefHeight="36.0" prefWidth="70.0" text="Back" />
